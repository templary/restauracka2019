<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="720.0" minWidth="1280.0" prefHeight="1080.0" prefWidth="1920.0" spacing="10" style="-fx-background-color: C1C8E4;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.app.restauracka.demo.UI.PridejJidloController">
    <padding>
        <Insets bottom="10.0" left="150.0" right="150.0" top="10.0" />
    </padding>
    <children>
        <TitledPane prefHeight="352.0" prefWidth="1520.0" style="-fx-background-color: C1C8E4;" text="Pridej jídlo do menu">
            <content>
                <VBox prefHeight="68.0" prefWidth="1518.0" spacing="10">
                    <children>
                        <HBox alignment="CENTER" spacing="10">
                            <children>
                                <Label prefHeight="17.0" prefWidth="80.0" text="Název">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                                <TextField fx:id="vytvorJidloNazev">
                           <font>
                              <Font size="18.0" />
                           </font></TextField>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10">
                            <children>
                                <Label prefHeight="17.0" prefWidth="80.0" text="Popis">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                                <TextField fx:id="vytvorJidloPopis">
                           <font>
                              <Font size="18.0" />
                           </font></TextField>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10">
                            <children>
                                <Label prefHeight="17.0" prefWidth="80.0" text="Cena">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                                <TextField fx:id="vytvorJidloCena">
                           <font>
                              <Font size="18.0" />
                           </font></TextField>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10" />
                        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="1500.0" spacing="10">
                            <children>
                                <Button fx:id="buttonVytvor" mnemonicParsing="false" onAction="#handleButtonVytvor" prefHeight="50.0" prefWidth="107.0" text="Vytvořit">
                           <font>
                              <Font size="18.0" />
                           </font></Button>
                                <Button fx:id="buttonSmazat" mnemonicParsing="false" onAction="#handleButtonSmazat" prefHeight="50.0" prefWidth="107.0" text="Smazat">
                           <font>
                              <Font size="18.0" />
                           </font></Button>
                            </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="90.0" />
                     </padding>
                        </HBox>
                    </children>
                </VBox>
            </content>
         <font>
            <Font size="20.0" />
         </font>
        </TitledPane>

        <TableView fx:id="tabulkaJidla" prefHeight="920.0" prefWidth="1520.0">
            <columns>
                <TableColumn fx:id="vytvorJidloTabulkaNazev" prefWidth="-1.0" text="Název">
                    <cellValueFactory>
                        <PropertyValueFactory property="firstName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="vytvorJidloTabulkaCena" prefWidth="159.0" text="Cena">
                    <cellValueFactory>
                        <PropertyValueFactory property="lastName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="vytvorJidloTabulkaID" prefWidth="92.0" text="ID">
                    <cellValueFactory>
                        <PropertyValueFactory property="age" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         <VBox.margin>
            <Insets />
         </VBox.margin>
        </TableView>

        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="1899.0" spacing="10">
            <children>
                <Button fx:id="registraceButtonZpet" onAction="#handleButtonZpet" prefHeight="50.0" prefWidth="107.0" text="Zpět">
               <font>
                  <Font size="18.0" />
               </font></Button>
            </children>
        </HBox>
    </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
